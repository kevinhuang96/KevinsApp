<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.calculator.Variables_Screen">

    <TextView
        android:id="@+id/MainScreen"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        app:layout_constraintLeft_toLeftOf="@+id/buttonA"
        app:layout_constraintRight_toRightOf="@+id/buttonDelClear"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonB" />

    <Button
        android:id="@+id/buttonDelClear"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/del_clear_button"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/buttonC"
        android:layout_marginLeft="0dp"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonC" />

    <Button
        android:id="@+id/buttonSTO"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="16dp"
        android:text="@string/Sto_function"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonG"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/buttonG"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/buttonK"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="16dp"
        android:text="@string/K_variable"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonJ"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/buttonJ"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/buttonO"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="16dp"
        android:text="@string/O_variable"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonN"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/buttonN"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/buttonS"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="16dp"
        android:text="@string/S_variable"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonR"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/buttonR"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/buttonW"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="16dp"
        android:text="@string/W_variable"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonV"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/buttonV"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/buttonEnter"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/Enter_button"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonZ"
        app:layout_constraintLeft_toRightOf="@+id/buttonZ"
        android:layout_marginLeft="0dp"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/buttonC"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:text="@string/C_variable"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonB"
        app:layout_constraintLeft_toRightOf="@+id/buttonB" />

    <Button
        android:id="@+id/buttonG"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:text="@string/G_variable"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonF"
        app:layout_constraintLeft_toRightOf="@+id/buttonF" />

    <Button
        android:id="@+id/buttonJ"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:text="@string/J_variable"
        android:textAppearance="@style/TextAppearance.AppCompat"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonI"
        app:layout_constraintLeft_toRightOf="@+id/buttonI" />

    <Button
        android:id="@+id/buttonN"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:text="@string/N_variable"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonM"
        app:layout_constraintLeft_toRightOf="@+id/buttonM"
        tools:ignore="UnknownId" />

    <Button
        android:id="@+id/buttonR"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:text="@string/R_variable"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonQ"
        app:layout_constraintLeft_toRightOf="@+id/buttonQ" />

    <Button
        android:id="@+id/buttonV"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:text="@string/V_variable"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonU"
        app:layout_constraintLeft_toRightOf="@+id/buttonU" />

    <Button
        android:id="@+id/buttonZ"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:text="@string/Z_variable"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonY"
        app:layout_constraintLeft_toRightOf="@+id/buttonY" />

    <Button
        android:id="@+id/buttonB"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:text="@string/B_variable"
        android:textAppearance="@style/TextAppearance.AppCompat"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonA"
        app:layout_constraintLeft_toRightOf="@+id/buttonA" />

    <Button
        android:id="@+id/buttonF"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:text="@string/F_variable"
        android:textAppearance="@style/TextAppearance.AppCompat"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonD"
        app:layout_constraintLeft_toRightOf="@+id/buttonD" />

    <Button
        android:id="@+id/buttonI"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:text="@string/I_variable"
        android:textAppearance="@style/TextAppearance.AppCompat"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonH"
        app:layout_constraintLeft_toRightOf="@+id/buttonH" />

    <Button
        android:id="@+id/buttonM"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:text="@string/M_variable"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonL"
        app:layout_constraintLeft_toRightOf="@+id/buttonL" />

    <Button
        android:id="@+id/buttonQ"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:text="@string/Q_variable"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonP"
        app:layout_constraintLeft_toRightOf="@+id/buttonP" />

    <Button
        android:id="@+id/buttonU"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:text="@string/U_variable"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonT"
        app:layout_constraintLeft_toRightOf="@+id/buttonT" />

    <Button
        android:id="@+id/buttonY"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="-7dp"
        android:text="@string/Y_variable"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonX"
        app:layout_constraintLeft_toRightOf="@+id/buttonX" />

    <Button
        android:id="@+id/buttonA"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="16dp"
        android:text="@string/A_variable"
        android:textAppearance="@style/TextAppearance.AppCompat"
        app:layout_constraintBottom_toTopOf="@+id/buttonD"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/buttonD"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="16dp"
        android:text="@string/D_variable"
        app:layout_constraintBottom_toTopOf="@+id/buttonH"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/buttonH"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="16dp"
        android:text="@string/H_variable"
        android:textAppearance="@style/TextAppearance.AppCompat"
        app:layout_constraintBottom_toTopOf="@+id/buttonL"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/buttonL"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="16dp"
        android:text="@string/L_variable"
        app:layout_constraintBottom_toTopOf="@+id/buttonP"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/buttonP"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="16dp"
        android:text="@string/P_variable"
        app:layout_constraintBottom_toTopOf="@+id/buttonT"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/buttonT"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="16dp"
        android:text="@string/T_variable"
        app:layout_constraintBottom_toTopOf="@+id/buttonX"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/buttonX"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="16dp"
        android:text="@string/X_variable"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />
</android.support.constraint.ConstraintLayout>
